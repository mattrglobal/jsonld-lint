{
  "name": "jsonld-lint-vscode",
  "description": "JSON-LD Linter for vs code",
  "private": true,
  "license": "Apache-2.0",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.46.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/index.js",
  "contributes": {
    "colors": [
      {
        "id": "jsonldlint.JsonLdLintResultBackgroundColor",
        "description": "Background color style for an issue identified by JSON-LD lint",
        "defaults": {
          "dark": "#e63333e5",
          "light": "#e63333e5",
          "highContrast": "#e63333e5"
        }
      }
    ]
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test:integration": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.46.1",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "3.7.5",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "jsonld-lint": "0.2.0"
  }
}
